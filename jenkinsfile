node {
    stage('SCM') {
	echo 'Gathering code from version control' 
	echo 'Test'
    }
    stage('Build') {
        echo 'Building....'
	withDotNet(sdk: "sdk6"){
	    sh 'dotnet --version'
	    sh 'dotnet build ConsoleApp1'
	    echo 'Build new feature'
	}
    }
    stage('Test') {
        echo 'Testing....'
    }
    stage('Deploy') {
        echo 'Deploying....'
    }
}
